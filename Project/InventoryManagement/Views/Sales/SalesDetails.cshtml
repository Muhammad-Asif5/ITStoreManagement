                                <textarea placeholder="Enter Comments" required="required" id="Comments" maxlength="100" name="Comments" data-plugin-maxlength="data-plugin-maxlength" class="form-control"></textarea>
                                <span class="field-validation-valid text-danger" data-valmsg-for="Comments" data-valmsg-replace="true"></span>
                            </div>
                        </div>
                    </div>
                    <button type="submit" id="btnReturn" class="btn btn-success">Return</button>

@model IEnumerable<InventoryManagement.Models.v_SalesDetails>

@{
    Layout = "~/Views/Shared/_Admin3.cshtml";
}

<br />
<div class="row">
    <div class="col-12">
        <div class="card card-primary card-outline">
            <div class="card-header">
                <h3 class="card-title">
                    <button type="button" class="btn btn-success" id="AddBtn">
                        <i class="fas fa-plus"></i> Add new
                    </button>
                </h3>
            </div>
            <div class="card-body">
                <h4 style="border-bottom:2px solid #ff6a00; text-align:center;">@Request.QueryString["EmpID"]</h4>
                <table id="myTable" class="table table-bordered table-hover">
                    <thead>
                        <tr>
                            <th style="width:25%">Product</th>
                            <th style="width:15%">Inventory No</th>
                            <th style="width:15%">Issued From</th>
                            <th style="width:10%">Type</th>
                            <th style="width:10%">Status</th>
                            <th style="width:25%"></th>
                        </tr>
                    </thead>
                    <tbody>
                        @{
                            int counter = 1;// Index start value
                            foreach (var item in Model)
                            {
                                <tr id="@item.Inventory_No@item.SerialNo">

                                    <td>
                                        @Html.DisplayFor(modelItem => item.CatName) @Html.DisplayFor(modelItem => item.Model_Name)
                                    </td>

                                    <td>
                                        @Html.DisplayFor(modelItem => item.Inventory_No)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Issued_From)
                                    </td>
                                    <td>
                                        @if (item.Issue_Type == "1")
                                        {
                                            <label class="badge bg-success">Permanent</label>
                                        }
                                        @if (item.Issue_Type == "0")
                                        {
                                            <label class="badge bg-warning">Temporary</label>
                                        }
                                    </td>
                                    <td>
                                        @if (item.SalesDetailsStatus == "Y")
                                        {
                                            <label class="badge bg-success">Issued</label>
                                        }
                                        @if (item.SalesDetailsStatus == "N")
                                        {
                                            <label class="badge bg-warning">Returned</label>
                                        }
                                    </td>
                                    @if (item.SalesDetailsStatus == "N")
                                    {
                                        <td>
                                            <button disabled class="btn  btn-outline-primary">Return</button>
                                            <button disabled onclick="GetSalesDetailsById(this.id)" id="@item.Inventory_No@item.SerialNo" class="btn btn-success">Edit</button>
                                            <button disabled onclick="DeleteSalesDetailsById(this.id)" id="@item.Inventory_No@item.SerialNo" class="btn btn-danger">Dalete</button>
                                        </td>
                                    }
                                    @if (item.SalesDetailsStatus == "Y")
                                    {
                                        <td>
                                            <button id="@item.Inventory_No@item.SerialNo" class="btn  btn-outline-primary">Return</button>
                                            <button onclick="GetSalesDetailsById(this.id)" id="@item.Inventory_No@item.SerialNo" class="btn btn-success">Edit</button>
                                            <button onclick="DeleteSalesDetailsById(this.id)" id="@item.Inventory_No@item.SerialNo" class="btn btn-danger">Dalete</button>
                                        </td>
                                    }


                                </tr>
                                counter++;
                            }
                        }


                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="DepartmentModal">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title"></h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form action="/Sales/AddOrEditsalesDetails" autocomplete="off" class="form-horizontal" enctype="multipart/form-data" id="myForm" method="post" role="form" novalidate="novalidate">
                    <div class="row">
                        <div class="col-sm-12">
                            <input data-val="true" hidden data-val-number="The field IssuedID must be a number." id="IssuedID" name="IssuedID" value="0">
                            <div class="form-group">
                                <label class="control-label" for="Inventory_No">Select Item</label>
                                <select id="Inventory_No" name="Inventory_No" class="form-control select2" required="required">
                                    <option value="">--Select Item--</option>
                                </select>
                                @*@Html.DropDownList("Inventory_No", (List<SelectListItem>)ViewBag.allAvailableInventory,
                                    "--Select Product--", new {@onchange="changeDDLFunction(this.id, this.value)", @class = "form-control select2", @required = "required" })*@
                                <span class="field-validation-valid text-danger" data-valmsg-for="Inventory_No" data-valmsg-replace="true"></span>
                            </div>
                        </div>

                        <div class="col-sm-12">
                            <div class="form-group">
                                <label class="control-label" for="Description">Description</label>
                                <textarea id="Description" maxlength="200" name="Description" placeholder="Enter Description Optional" class="form-control text-box single-line" data-plugin-maxlength="data-plugin-maxlength"></textarea>

                                <span class="field-validation-valid text-danger" data-valmsg-for="Description" data-valmsg-replace="true"></span>
                            </div>
                        </div>
                        <div class="col-sm-12">
                            <div class="form-group">
                                <label class="control-label" for="Issued_From">Issued_From</label>
                                <input class="form-control text-box single-line" data-plugin-maxlength="data-plugin-maxlength" id="Issued_From" maxlength="150" name="Issued_From" placeholder="Enter Issued_From" required="required" type="text" value="">
                                <span class="field-validation-valid text-danger" data-valmsg-for="Issued_From" data-valmsg-replace="true"></span>
                            </div>
                        </div>
                        <div class="col-sm-12">
                            <div class="form-group">
                                <label class="control-label" for="Issue_Type">Issue_Type</label>
                                <select class="form-control" id="Issue_Type" name="Issue_Type" required="required">
                                    <option selected="selected" value="">--Select Issue_Type--</option>
                                    <option value="1">Permanent</option>
                                    <option value="0">Temporary</option>
                                </select>
                                <span class="field-validation-valid text-danger" data-valmsg-for="Issue_Type" data-valmsg-replace="true"></span>
                            </div>
                        </div>
                        @*<div class="col-sm-6">
                                <div class="form-group">
                                    <label class="control-label" for="Status">Status</label>
                                    <select class="form-control" id="Status" name="Status" required="required">
                                        <option selected="selected" value="">--Select Status--</option>
                                        <option value="1">Permanent</option>
                                        <option value="0">Temporary</option>
                                    </select>
                                    <span class="field-validation-valid text-danger" data-valmsg-for="Issue_Type" data-valmsg-replace="true"></span>
                                </div>
                            </div>*@
                    </div>
                    <button type="submit" id="btnSave" class="btn btn-success">Add Sales</button>
                    <button type="submit" id="btnUpdate" class="btn btn-primary" style="display: none;">Update Sales</button>
                </form>
            </div>

        </div>
        <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
</div>

<div class="modal fade" id="receivedMpdal">
